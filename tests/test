int i;
int j;
int t;
int a[100]; 
int size;

   

size = atoi(input());
while (j < size) {
    a[j] = atoi(input());
    j = j + 1;
}

i=0;
j=size-1;
while (i < j) {
    t = a[i];
    a[i] = a[j];
    a[j] = t;
    i = i+1;
    j = j-1;
}

i=0;
while (i < size) {
    printi(a[i]);
    println();
    i = i+1;
}

